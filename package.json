{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.2.6",
    "@ant-design/pro-card": "^1.11.4",
    "@ant-design/pro-descriptions": "^1.6.12",
    "@ant-design/pro-form": "^1.34.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-list": "^1.12.0",
    "@ant-design/pro-table": "^2.34.5",
    "@tinymce/tinymce-react": "^3.12.2",
    "@types/lodash.template": "^4.5.0",
    "@umijs/preset-react": "^1.x",
    "lodash.template": "^4.5.0",
    "react-amap": "^1.2.8",
    "react-amap-plugin-autocomplete": "^0.0.4",
    "umi": "^3.4.7"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-qiankun": "^2.26.0",
    "@umijs/test": "^3.4.7",
    "husky": "^4.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
